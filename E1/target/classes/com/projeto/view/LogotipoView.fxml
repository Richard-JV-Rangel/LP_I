<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.projeto.controler.LogotipoController">
    <children>
        <VBox alignment="TOP_CENTER" spacing="15.0" AnchorPane.topAnchor="20.0"
            AnchorPane.rightAnchor="20.0" AnchorPane.leftAnchor="20.0">
            <Label styleClass="title-label" text="Modelagem: Interações de Logotipo" />
            <HBox alignment="CENTER" spacing="20.0">
                <VBox alignment="TOP_CENTER" spacing="10.0" styleClass="section-pane">
                    <Label styleClass="section-title-label" text="Classe: Simbolo" />
                    <Button onAction="#handleAnimarSimbolo" text="Executar animar()" />
                    <Button onAction="#handleMudarCorSimbolo" text="Executar mudarCor()" />
                    <Button onAction="#handleRepresentarMarca" text="Executar representarMarca()" />
                </VBox>
                <VBox alignment="TOP_CENTER" spacing="10.0" styleClass="section-pane">
                    <Label styleClass="section-title-label" text="Classe: Mao" />
                    <Button onAction="#handleClicarMao" text="Executar clicar()" />
                    <Button onAction="#handleCurtirMao" text="Executar curtir()" />
                    <Button onAction="#handleDigitarMao" text="Executar digitar()" />
                </VBox>
            </HBox>
            <Label styleClass="section-title-label" text="Console de Resultados:">
                <VBox.margin>
                    <Insets top="10.0" />
                </VBox.margin>
            </Label>
            <TextArea fx:id="outputArea" editable="false" prefHeight="250.0" wrapText="true" />
        </VBox>
    </children>
</AnchorPane>